{
  "name": "barotrauma-circuits",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env -- vite",
    "build": "dotenvx run -- vite build",
    "preview": "dotenvx run -- vite preview",
    "test": "dotenvx run -f .env.test -- vitest run --coverage",
    "test:unit": "dotenvx run -f .env.test -- vitest",
    "test:e2e": "dotenvx run -f .env.test -- cypress run",
    "e2e": "cypress open",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "cov:combine": "nyc merge --dir coverage --include coverage/** combined-coverage/coverage.json && npm run cov:report",
    "cov:report": "nyc report -e .js -e .vue --include src/**/*.{js,ts,vue} --exclude src/components/icons/**/*.{js,ts,vue} --reporter=html --reporter=text -t combined-coverage --report-dir=combined-coverage",
    "test:all": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./combined-coverage && npm run test && npm run test:e2e && npm run cov:combine"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.43",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.5.1",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "lcov-result-merger": "^5.0.1",
    "nyc": "^17.1.0",
    "vite": "^5.4.19",
    "vite-plugin-istanbul": "^7.0.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.45.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@vitejs/plugin-vue": "^6.0.0",
    "bootstrap": "^5.3.3",
    "cypress-real-events": "^1.14.0",
    "pako": "^2.1.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-toastification": "^2.0.0-rc.5"
  }
}
